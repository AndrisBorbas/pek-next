    <tr>
      <td> <%= m.usr_lastname %> <%= m.usr_firstname %> </td>
      <td> <%= m.usr_nickname %> </td>
      <td> <%= m.usr_dormitory %> </td>
      <td> <%= m.usr_room %> </td>
      <td> <%= m.usr_email %> </td>
    </tr>
  <% end %>
  </tbody>
  <tfoot>
    <tr>
      <td><a href="#">Többi megjelenítése</a></td>
    </tr>
  </tfoot>

</table>

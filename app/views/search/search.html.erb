
<table class="uk-table uk-table-striped uk-table-hover">
  <thead>
    <tr>
      <th>Név</th>
      <th>Becenév</th>
    </tr>
  </thead>
  <tbody id="suggestions">
  <% Array(@UsersNameFirstX).each do |m| %>
    <tr>
      <td> <%= m.usr_lastname %> <%= m.usr_firstname %> </td>
      <td> <%= m.usr_nickname %> </td>
    </tr>
  <% end %>
  <% Array(@UsersNickFirstX).each do |m| %>
    <tr>
      <td> <%= m.usr_lastname %> <%= m.usr_firstname %> </td>
      <td> <%= m.usr_nickname %> </td>
    </tr>
  <% end %>
  </tbody>
  <tfoot>
    <tr>
      <td><a href="#">Többi megjelenítése</a></td>
    </tr>
  </tfoot>
</table>

<table class="uk-table uk-table-striped uk-table-hover">
